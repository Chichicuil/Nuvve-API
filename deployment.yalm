apiVersion: apps/v1
kind: Deployment
metadata:
  name: electric-vehicle-service
spec:
  replicas: 1
  selector:
    matchLabels:
      app: electric-vehicle-service
  template:
    metadata:
      labels:
        app: electric-vehicle-service
    spec:
      containers:
      - name: electric-vehicle-service
        image: gcr.io/[PROJECT_ID]/[IMAGE_NAME]:[TAG]
        ports:
        - containerPort: 8080
